<Window x:Class="GruppeArbejdeOrg.Windows.ProjectSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GruppeArbejdeOrg.Windows"
        mc:Ignorable="d"
        Title="Project Settings" Height="300" Width="300">
    <Grid>
        <Label Content="Project Settings" Margin="61,25,61,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" FontSize="18"/>
        <Separator Height="15" Margin="28,64,28,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ProjectName" Height="23" Margin="28,84,28,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" RenderTransformOrigin="0.681,-0.101"/>
        <Button Content="Apply Settings" Margin="28,112,28,0" VerticalAlignment="Top" Click="ApplySettings"/>

    </Grid>
</Window>
